        android:textAlignment="center"
        android:textSize="42sp"
        app:layout_constraintBottom_toBottomOf="@+id/running_tab"
        app:layout_constraintEnd_toEndOf="@+id/running_tab"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="@+id/running_tab"
        app:layout_constraintTop_toTopOf="@+id/running_tab"
