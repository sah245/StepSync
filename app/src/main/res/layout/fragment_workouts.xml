        android:textSize="42sp"
        app:layout_constraintBottom_toBottomOf="@+id/cycling_tab"
        app:layout_constraintEnd_toEndOf="@+id/cycling_tab"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="@+id/cycling_tab"
        app:layout_constraintTop_toTopOf="@+id/cycling_tab"
        app:layout_constraintVertical_bias="0.242" />

    <TextView
        android:id="@+id/lift_count_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textAlignment="center"
        android:textSize="42sp"
        app:layout_constraintBottom_toBottomOf="@+id/lifting_tab"
        app:layout_constraintEnd_toEndOf="@+id/lifting_tab"
        app:layout_constraintHorizontal_bias="0.535"
        app:layout_constraintStart_toStartOf="@+id/lifting_tab"
        app:layout_constraintTop_toTopOf="@+id/lifting_tab"
        app:layout_constraintVertical_bias="0.242" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="@+id/running_tab"
        app:layout_constraintEnd_toEndOf="@+id/running_tab"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintStart_toStartOf="@+id/running_tab"
        app:layout_constraintTop_toTopOf="@+id/running_tab"
        app:layout_constraintVertical_bias="1.0"
        app:strokeColor="#5A9754" />

